{
    "manifest_version": 3,
    "name": "AJ PR Review",
    "version": "1.0",
    "description": "Toggle snap.js file from the webpage.",
    "permissions": [
        "contextMenus",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [ {
        "matches": [ "http://*/*", "https://*/*"],
        "match_origin_as_fallback": true,
        "js": [
            "js/jquery-3.6.0.min.js"
        ]
    } ],
    "background": {
        "service_worker": "js/background.js"
    },
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    }
}
